# Test environment configuration
NODE_ENV=test

# Clear DATABASE_URL so PG* variables take precedence
# (root .env might have DATABASE_URL with different credentials)
DATABASE_URL=

# Database connection for tests - uses Docker database
# Connect to the main database first to create the test database
PGHOST=localhost
PGPORT=5432
PGUSER=postgres
PGPASSWORD=postgres
PGDATABASE=qualitative_canvas
PGDATABASE_DEV=qualitative_canvas
TEST_DB_NAME=qualitative_canvas_test

# API Configuration
PORT=5002
